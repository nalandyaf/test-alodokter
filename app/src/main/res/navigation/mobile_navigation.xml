<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.test.alodokter.ui.main.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/to_detail_image"
            app:destination="@id/navigation_detail_home" />

    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.test.alodokter.ui.main.ui.profile.ProfileFragment"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_detail_home"
        android:name="com.test.alodokter.ui.main.ui.home.detail.ImageDetailFragment"
        tools:layout="@layout/fragment_detail_image" />

</navigation>